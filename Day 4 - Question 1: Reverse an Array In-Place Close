#include <stdio.h>

int main() {
    int n, i;
    int a[100];

  
    scanf("%d", &n);

    
    for (i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    int left = 0;
    int right = n - 1;
    int temp;

    while (left < right) {
    
        temp = a[left];
        a[left] = a[right];
        a[right] = temp;

        left++;
        right--;
    }


    for (i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }

    return 0;
}
